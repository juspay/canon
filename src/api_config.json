{
  "placeholder": {
    "order_id": "$randomId",
    "api_key": "Basic Mxxxxxxxxxx==",
    "merchant_id": "paypal",
    "amount": "$randomInt",
    "customer_id": "$randomId",
    "payStartUrl": "@api~txns~response~payment~authentication~url"
  },
  "apiOrder": [
    "order-create",
    "txns",
    "payStart",
    "orderStatus"
  ],
  "api": {
    "order-create": {
      "endpoint": "https://sandbox.juspay.in/orders",
      "method": "POST",
      "contentType": "URLFORMENCODED",
      "headers": {
        "Authorization": "#api_key",
        "x-merchantid": "#merchant_id"
      },
      "request": {
        "order_id": "#order_id",
        "amount": "#amount",
        "customer_id": "#customer_id",
        "currency": "INR",
        "customer_email": "abc@ac.com",
        "customer_phone": "7899438420"
      }
    },
    "txns": {
      "endpoint": "https://sandbox.juspay.in/txns",
      "method": "POST",
      "contentType": "URLFORMENCODED",
      "headers": {
        "x-merchantid": "#merchant_id"
      },
      "request": {
        "order_id": "#order_id",
        "merchant_id": "#merchant_id",
        "payment_method_type": "CARD",
        "payment_method": "VISA",
        "card_number": "4111111111111111",
        "card_exp_month": "02",
        "card_exp_year": "25",
        "name_on_card": "testing",
        "card_security_code": "926",
        "format": "json",
        "redirect_after_payment": "true"
      }
    },
    "payStart": {
      "endpoint": "#payStartUrl",
      "method": "GET",
      "headers": {
        "x-merchantid": "#merchant_id"
      },
      "request": {}
    },
    "orderStatus": {
      "endpoint": "https://sandbox.juspay.in/ecr/orderStatus",
      "method": "POST",
      "contentType": "URLFORMENCODED",
      "headers": {
        "Authorization": "#api_key",
        "x-merchantid": "#merchant_id"
      },
      "request": {
        "order_id": "#order_id"
      }
    }
  }
}